<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Application_E2A.Projects" >

    <!--BASE-->
    <Style x:Key ="BaseCheckBoxStyle" TargetType="{x:Type CheckBox}">
    </Style>

    <!--PENDING-->
    <Style x:Key ="CheckBox_Pending" TargetType="{x:Type CheckBox}" BasedOn="{StaticResource BaseCheckBoxStyle}">
    </Style>

</ResourceDictionary>
